{
    "name": "chatter-laravel/core",
    "description": "A simple forum package for your laravel app",
    "license": "MIT",
    "authors": [
        {
            "name": "Santiago Barbat",
            "email": "barbatsan@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1.0",
        "mews/purifier": "^3.1",
        "cviebrock/eloquent-sluggable": "^8.0",
        "laravel-frontend-presets/tailwindcss": "^4.1",
        "laravel/ui": "^3.0",
        "kg-bot/laravel-localization-to-vue": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.0",
        "laravel/sail": "^1.12"
    },
    "autoload": {
	    "psr-4": {
	        "Chatter\\Core\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Chatter\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chatter\\Core\\ChatterServiceProvider"
            ]
        }
    }
}
